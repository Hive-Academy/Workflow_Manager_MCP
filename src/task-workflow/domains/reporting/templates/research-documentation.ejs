                    { name: 'User Manual', status: 'partial' },
                    { name: 'Getting Started Guide', status: 'complete' },
                    { name: 'FAQ Section', status: 'missing' },
                    { name: 'Video Tutorials', status: 'missing' },
                    { name: 'Troubleshooting Guide', status: 'partial' }
                ]
            },
